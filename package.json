{
  "name": "unplugin-actions-kit",
  "version": "0.0.0",
  "description": "A toolkit for GitHub Actions",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.12.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/unplugin-actions-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/unplugin-actions-kit.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/unplugin-actions-kit/issues"
  },
  "keywords": [
    "actions",
    "github actions",
    "toolkit",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "rolldown",
    "rspack"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./esbuild": {
      "import": {
        "types": "./dist/esbuild.d.ts",
        "default": "./dist/esbuild.mjs"
      },
      "require": {
        "types": "./dist/esbuild.d.cts",
        "default": "./dist/esbuild.cjs"
      }
    },
    "./rolldown": {
      "import": {
        "types": "./dist/rolldown.d.ts",
        "default": "./dist/rolldown.mjs"
      },
      "require": {
        "types": "./dist/rolldown.d.cts",
        "default": "./dist/rolldown.cjs"
      }
    },
    "./rollup": {
      "import": {
        "types": "./dist/rollup.d.ts",
        "default": "./dist/rollup.mjs"
      },
      "require": {
        "types": "./dist/rollup.d.cts",
        "default": "./dist/rollup.cjs"
      }
    },
    "./rspack": {
      "import": {
        "types": "./dist/rspack.d.ts",
        "default": "./dist/rspack.mjs"
      },
      "require": {
        "types": "./dist/rspack.d.cts",
        "default": "./dist/rspack.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.mjs"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./webpack": {
      "import": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.mjs"
      },
      "require": {
        "types": "./dist/webpack.d.cts",
        "default": "./dist/webpack.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@actions/core": "^1.11.1",
    "@rspack/core": ">=1.0.0",
    "esbuild": ">=0.24.0",
    "rolldown": "0.12.1",
    "rollup": ">=3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.2",
    "js-yaml": "^4.1.0",
    "unplugin": "^1.14.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@biomejs/biome": "^1.9.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rspack/core": "1.0.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.16.11",
    "esbuild": "^0.24.0",
    "rolldown": "0.13.2",
    "rollup": "^4.24.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.8.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "vitest-testdirs": "^1.1.0",
    "webpack": "^5.95.0"
  }
}
